<?xml version="1.0" encoding="utf-8"?>
<manifest
    xmlns:android="http://schemas.android.com/apk/res/android"
    package="mx.edu.utng.operationsbasic">

    <application
        android:allowBackup="true"
        android:icon="@drawable/multiplicacion"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".SumaActivity"/>
        <activity android:name=".RestaActivity"/>
        <activity android:name=".MultiplicacionActivity"/>
        <activity android:name=".DivisionActivity"/>
    </application>

</manifest>